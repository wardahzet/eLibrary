<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ELibrary"
        tools:targetApi="31">

        <activity
            android:name=".layout_activity.rent.RentStatusActivity"
            android:exported="false" />
        <activity
            android:name=".layout_activity.rent.CheckoutActivity"
            android:exported="false" />
        <activity
            android:name=".layout_activity.book_detail.DetailActivity"
            android:exported="false" />
        <activity
            android:name=".layout_activity.auth.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".layout_activity.Home"
            android:exported="false"/>
        <activity
            android:name=".layout_activity.auth.LoginPage"
            android:exported="false" />
        <activity
            android:name=".layout_activity.auth.RegisterPage"
            android:exported="false" />
        <activity
            android:name=".layout_activity.auth.OnBoarding"
            android:exported="false" />
        <activity
            android:name=".layout_activity.wishlist.WishlistActivity"
            android:exported="false" />
        <activity
            android:name=".layout_activity.profile.ProfileActivity"
            android:exported="false" />
        <activity
            android:name=".layout_activity.profile.EditProfile"
            android:exported="false" />
        <activity
            android:name=".layout_activity.history.HistoryActivity"
            android:exported="false" />
    </application>


</manifest>